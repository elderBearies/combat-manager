{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
		"no-unused-vars": 1,
		"no-underscore-dangle" : "off",
        "no-plusplus": "off",
        "import/extensions": "off"
    }
}
